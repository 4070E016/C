/* prog8_12,??? */
#include <stdio.h>
#include <stdlib.h>
int fib(int);			/* fib()ㄧ? */
int main(void)
{
   int n,x;
   printf("請輸入你要計算的內容:");
   scanf("%d",&x);
   for(n=1;n<=x;n++)			/* 璸衡玡10??? */
      printf("fib(%d)=%d\n",n,fib(n));		

   system("pause");
   return 0;
}

int fib(int n) 		/* ﹚?ㄧ?fib()璸衡???材n? */
{	
   if(n==1 || n==2)	/* 狦n=1?n=2玥肚1 */
      return 1;
   else				/* 玥肚 fib(n-1)+fib(n-2) */
      return (fib(n-1)+fib(n-2));
}




/* prog8_12,??? */
#include <stdio.h>
#include <stdlib.h>
int fib(int);			/* fib()ㄧ? */
int main(void)
{
   int n,x;
   printf("請輸入你要計算的內容:");
   scanf("%d",&x);
   n=1;
   do
      {
	  printf("fib(%d)=%d\n",n,fib(n));		
      n++;
       }
	  while(n<=x);

 
	

   system("pause");
   return 0;
}

int fib(int n) 	
{	
   if(n==1 || n==2)	
      return 1;
   else				
      return (fib(n-1)+fib(n-2));
}
